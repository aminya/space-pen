{
  "name": "space-pen-plus",
  "version": "5.2.0",
  "description": "modern spece-pen",
  "main": "./lib/space-pen.js",
  "browser": "./dist/space-pen.js",
  "es": "./es/space-pen.js",
  "files": [
    "src/**",
    "lib/**",
    "es/**",
    "dist/**"
  ],
  "scripts": {
    "test-build": "coffee -c -M -o spec spec",
    "test": "npm run test-build && atom --test spec",
    "start": "grunt start",
    "clean": "shx rm -rf lib && shx rm -rf dist && shx rm -rf es &&  shx rm -f spec/*.js && rm -f api.json",
    "debug": "npm run clean && shx cp -r src lib && coffee -c -M -o lib src && shx rm -r lib/*.coffee",
    "dev": "npm run clean && env NODE_ENV=development rollup -c -w",
    "build": "npm run clean && env NODE_ENV=production rollup -c && shx cp -r lib dist",
    "prepare": "npm run build"
  },
  "atomTestRunner": "./runner",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/aminya/space-pen/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aminya/space-pen.git"
  },
  "bugs": {
    "url": "https://github.com/aminya/space-pen/issues"
  },
  "devDependencies": {
    "coffeescript": "1.12.7",
    "rollup": "^2.11.2",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-coffee-script": "^2.0.0",
    "shx": "^0.3.2",
    "atom-jasmine3-test-runner": "4.4.7",
    "jasmine": "^3.5.0",
    "@types/jasmine": "^3.5.10",
    "underscore-plus": "^1.7.0",
    "express": "~3.4.0",
    "grunt-cli": "^1.3.2",
    "grunt": "^1.1.0",
    "grunt-coffeelint": "^0.0.16",
    "browserify": "^16.5.1",
    "grunt-shell": "^3.0.1",
    "coffeeify": "^3.0.1",
    "grunt-contrib-connect": "^2.1.0",
    "grunt-atomdoc": "^1.0.1"
  },
  "dependencies": {
    "jquery": "^3.5.1"
  }
}
