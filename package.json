{
  "name": "space-pen",
  "version": "5.1.2",
  "description": "Write markup on the final frontier",
  "main": "./lib/space-pen.js",
  "browser": "./dist/space-pen.js",
  "es": "./es/space-pen.js",
  "files": [
    "src/**",
    "lib/**",
    "es/**",
    "dist/**"
  ],
  "scripts": {
    "test-build": "coffee -c -M -o spec spec",
    "test": "grunt test",
    "start": "grunt start",
    "clean": "shx rm -rf lib && shx rm -rf dist && shx rm -rf es &&  shx rm -f spec/*.js && rm -f api.json",
    "debug": "npm run clean && shx cp -r src lib && coffee -c -M -o lib src && shx rm -r lib/*.coffee",
    "dev": "npm run clean && env NODE_ENV=development rollup -c -w",
    "build": "npm run clean && env NODE_ENV=production rollup -c && shx cp -r lib dist",
    "prepare": "npm run build"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://github.com/atom/space-pen/raw/master/LICENSE"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/atom/space-pen.git"
  },
  "bugs": {
    "url": "https://github.com/atom/space-pen/issues"
  },
  "devDependencies": {
    "coffeescript": "1.12.7",
    "rollup": "^2.11.2",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-coffee-script": "^2.0.0",
    "shx": "^0.3.2",
    "express": "~3.4.0",
    "coffee-script": "~1.6.3",
    "grunt-contrib-coffee": "~0.7.0",
    "grunt-cli": "~0.1.8",
    "grunt": "~0.4.1",
    "grunt-coffeelint": "0.0.6",
    "browserify": "~2.34.1",
    "jasmine-focused": "~0.15.0",
    "grunt-shell": "~0.5.0",
    "coffeeify": "~0.5.1",
    "grunt-contrib-connect": "~0.5.0",
    "grunt-atomdoc": "^1.0.0"
  },
  "dependencies": {
    "jquery": "2.2.4"
  }
}
